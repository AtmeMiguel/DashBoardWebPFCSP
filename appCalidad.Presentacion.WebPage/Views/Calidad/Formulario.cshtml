                for (var obj in this.CAMPOS) {
                    if (this.CAMPOS[obj].VALIDAR == 1 && (`${this.CAMPOS[obj].value}`.length < 1 || `${this.CAMPOS[obj].value}` == 'Seleccionar' || `${this.CAMPOS[obj].value}` == 'SELECCIONAR')) {
