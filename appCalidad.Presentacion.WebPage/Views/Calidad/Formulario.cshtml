                this.show = true;
                let CAMPOS = '';
                let Plan = '';
                for (var obj in this.CAMPOS) {
                    let val = `${this.CAMPOS[obj].value}`;
                    val = val.replace("'", " ");
                    CAMPOS = CAMPOS + `${this.CAMPOS[obj].value_}` + '=|' + val + ',|';
                    Plan = Plan + `${this.CAMPOS[obj].NOMBRE}` + ':' + val + '|';
